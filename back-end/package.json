    "@types/node": "^11.13.5",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.1.0",

    "lint": "tslint './src/**/*.ts'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.instinctools.com/bantsevich.vitali/trello.git"
  },
  "keywords": [
    "trello",
    "work-flow-managment"
  ],
  "author": "Bantsevich team",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^4.2.0",
    "@types/dotenv": "^6.1.1",
    "@types/graphql": "^14.2.0",
    "@types/koa-router": "^7.0.40",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "chai-http": "^4.2.1",
    "dotenv": "^7.0.0",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.16.5",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "^0.0.16",
    "koa-graphql": "^0.8.0",

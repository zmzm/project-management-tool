{
  "name": "back-end",
  "version": "1.0.0",
  "description": "back-end for trello on steroids",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node dist/index.js",
    "start-dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "eslint": "./node_modules/.bin/eslint src/. --ext ts",
    "ts": "tsc",
    "test": "tsc && jest",
    "precommit": "npm run eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.instinctools.com/bantsevich.vitali/trello.git"
  },
  "keywords": [
    "trello",
    "work-flow-managment"
  ],
  "author": "Bantsevich team",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "knex": "^0.15.1",
    "koa": "^2.5.1",
    "koa-graphql": "^0.7.5",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.10",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "@types/node": "^10.5.2",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "nodemon": "^1.17.5",
    "sinon": "^6.1.2",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^16.0.1"
  }
}

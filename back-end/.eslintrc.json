{
  "extends": "airbnb-base",
  "parser": "typescript-eslint-parser",
  "plugins": ["typescript", "jest"],
  "rules": {
    "no-console": 1,
    "strict": 0,
    "no-param-reassign": [2, { "props": false }],
    "no-unused-vars": "off",
    "no-undef": "off"
  },
  "env": {
    "jest/globals": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".ts",
          ".tsx"
        ]
      }
    }
  }
}